#include "multiwhaleTestApp.h"
#include "multiwhaleApp.h"
#include "Moose.h"
#include "AppFactory.h"
#include "MooseSyntax.h"

InputParameters
multiwhaleTestApp::validParams()
{
  InputParameters params = multiwhaleApp::validParams();
  return params;
}

multiwhaleTestApp::multiwhaleTestApp(InputParameters parameters) : MooseApp(parameters)
{
  multiwhaleTestApp::registerAll(
      _factory, _action_factory, _syntax, getParam<bool>("allow_test_objects"));
}

multiwhaleTestApp::~multiwhaleTestApp() {}

void
multiwhaleTestApp::registerAll(Factory & f, ActionFactory & af, Syntax & s, bool use_test_objs)
{
  multiwhaleApp::registerAll(f, af, s);
  if (use_test_objs)
  {
    Registry::registerObjectsTo(f, {"multiwhaleTestApp"});
    Registry::registerActionsTo(af, {"multiwhaleTestApp"});
  }
}

void
multiwhaleTestApp::registerApps()
{
  registerApp(multiwhaleApp);
  registerApp(multiwhaleTestApp);
}

/***************************************************************************************************
 *********************** Dynamic Library Entry Points - DO NOT MODIFY ******************************
 **************************************************************************************************/
// External entry point for dynamic application loading
extern "C" void
multiwhaleTestApp__registerAll(Factory & f, ActionFactory & af, Syntax & s)
{
  multiwhaleTestApp::registerAll(f, af, s);
}
extern "C" void
multiwhaleTestApp__registerApps()
{
  multiwhaleTestApp::registerApps();
}
